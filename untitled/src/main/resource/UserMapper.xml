<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
在mapper中配置无数个sql语句
 namespace:命名空间，用于隔离sql语句
 namespace在mapper代理开发中有重要作用
 -->
<mapper namespace="com.eshare.mapper.UserMapper">
<insert id="save" parameterType="com.eshare.mapper.User">
   insert into tb_at_user(id,name,password) values(#{id},#{name},#{password})

</insert>


</mapper>